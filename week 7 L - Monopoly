#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a[4];
	    for(int i=0;i<4;i++)
	    {
	        cin>>a[i];
	    }
	    
	    sort(a,a+4);
	    
	    int ans=0;
	    
	    for(int i=0;i<3;i++)
	    ans+=a[i];
	    
	    if(ans<a[3])
	    cout<<"YES\n";
	    else
	    cout<<"NO\n";
	}
	return 0;
}
