#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin>>t;
	while(t--)
	{
	    int x;
	    int sum=0;
	    
	    for(int i=0;i<3;i++)
	    cin>>x,sum+=x;
	    
	    if(sum<=1)
	    cout<<"Water filling time"<<endl;
	    else
	    cout<<"Not now"<<endl;
	    
	    
	}
	return 0;
}
