void solve_the_problem(int test_case)
{
    string s;
    cin >> s;

    string ss = "314159265358979323846264338327";
    int cnt = 0;
    for (int i = 0; i < s.size(); i++)
    {
        if (s[i] == ss[i])
        {
            cnt++;
        }
        else
        {
            break;
        }
    }

    cout << cnt << endl;
}
